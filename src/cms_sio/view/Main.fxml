<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="cms_sio.controllers.MainController">
  <BorderPane id="AnchorPane" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <center>
      <AnchorPane fx:id="body" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER">
        <BorderPane.margin>
          <Insets />
        </BorderPane.margin>
      </AnchorPane>
    </center>
    <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <top>
      <HBox prefHeight="-1.0" prefWidth="-1.0">
        <children>
          <Button fx:id="page" mnemonicParsing="false" text="Page" />
          <Button fx:id="data" mnemonicParsing="false" text="Data" />
          <Button fx:id="template" mnemonicParsing="false" onAction="#showTemplate" text="Template" />
          <StackPane alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="600.0">
            <children>
              <Button fx:id="configuration" mnemonicParsing="false" onAction="#showConfiguration" text="">
                <graphic>
                  <ImageView fitHeight="24.0" fitWidth="24.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                    <image>
                      <Image url="@../resources/setting.png" />
                    </image>
                  </ImageView>
                </graphic>
              </Button>
            </children>
          </StackPane>
        </children>
      </HBox>
    </top>
  </BorderPane>
</AnchorPane>
