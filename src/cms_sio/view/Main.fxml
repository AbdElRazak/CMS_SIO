<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AnchorPane" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="cms_sio.controllers.MainController">
  <center>
    <FlowPane fx:id="body" minHeight="600.0" minWidth="650.0" prefHeight="-1.0" prefWidth="-1.0" />
  </center>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
  <top>
    <HBox prefHeight="45.0" prefWidth="600.0">
      <children>
        <Button fx:id="page" mnemonicParsing="false" text="Page" />
        <Button fx:id="data" mnemonicParsing="false" text="Data" />
        <Button fx:id="template" mnemonicParsing="false" onAction="#showTemplate" text="Template" />
        <StackPane alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="416.0">
          <children>
            <Button fx:id="configuration" mnemonicParsing="false" onAction="#showConfiguration" text="">
              <graphic>
                <ImageView fitHeight="24.0" fitWidth="24.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@../resources/setting.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
          </children>
        </StackPane>
      </children>
    </HBox>
  </top>
</BorderPane>
